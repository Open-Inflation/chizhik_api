{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "count": {
      "type": "integer"
    },
    "next": {
      "type": "null"
    },
    "previous": {
      "type": "null"
    },
    "page_size": {
      "type": "integer"
    },
    "total_pages": {
      "type": "integer"
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "distance": {
            "type": "number"
          },
          "has_shop": {
            "type": "boolean"
          },
          "fias_id": {
            "format": "uuid",
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "lon": {
            "type": "number"
          },
          "lat": {
            "type": "number"
          },
          "slug": {
            "type": "string"
          }
        },
        "required": [
          "distance",
          "fias_id",
          "has_shop",
          "name"
        ]
      }
    }
  },
  "required": [
    "count",
    "items",
    "next",
    "page_size",
    "previous",
    "total_pages"
  ]
}